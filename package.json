  "scripts": {
    "start:fishing-map": "nx start fishing-map",
    "start:vessel-history": "nx start vessel-history",
    "start:deck-playground": "nx start deck-playground",
    "start:real-time-prototype": "nx start real-time-prototype",
    "start:tile-inspector": "nx start tile-inspector",
    "run:fishing-map": "nx serve:export fishing-map",
    "run:fourwings-explorer": "nx serve:export fourwings-explorer",
    "build": "nx build",
    "test": "nx test",
    "dist": "nx run-many --target=dist --all --with-deps",
    "graph": "yarn nx dep-graph",
    "lint": "eslint . -c .eslintrc.js --ext .js,jsx,ts,tsx --fix",
    "deploy": "nx affected --target=deploy",
    "affected": "nx show projects --affected --base=origin/develop~1 --exclude='tile-inspector,*-e2e,libs/*' --plain > affected-apps.txt",
    "affected:libs": "nx show projects --affected --base=origin/develop~1 --exclude='apps/*' --plain > affected-libs.txt",
    "graph:affected": "yarn nx affected:dep-graph",
    "clean:deps": "rm -rf node_modules && rm yarn.lock && yarn install",
    "clean:dist": "rm -rf dist",
    "clean": "yarn clean:deps && yarn clean:dist",
    "prepare": "husky install"
  },
