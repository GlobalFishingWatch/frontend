  "license": "MIT",
  "author": "satellitestudio <contact@satellitestud.io>",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "linting"
    ]
  },
  "scripts": {
    "start:fishing-map": "nx start fishing-map",
    "start:vessel-history": "nx start vessel-history",
    "start:tile-inspector": "nx start tile-inspector",
    "build:fishing-map": "nx build fishing-map --parallel2",
    "run:fishing-map": "nx serve:export fishing-map",
    "build": "nx build",
    "test": "nx test",
    "dist": "nx run-many --target=dist --all --with-deps",
    "graph": "yarn nx dep-graph",
    "lint": "eslint . -c .eslintrc.js --ext .js,jsx,ts,tsx --fix",
    "deploy": "nx affected --target=deploy",
    "affected": "nx affected:apps nx --base=origin/develop~1 --exclude=tile-inspector --plain > affected-apps.txt",
    "affected:libs": "nx affected:libs --base=origin/develop~1 --exclude=api-client,api-types,data-transforms,dataviews-client,fourwings-aggregate,layer-composer,ocean-areas,pbf,timebar,react-hooks,ui-components --plain > affected-libs.txt",
    "graph:affected": "yarn nx affected:dep-graph",
    "clean:deps": "rm -rf node_modules/.yarn-integrity && yarn install",
    "clean:dist": "rm -rf dist",
    "clean": "yarn clean:deps && yarn clean:dist",
    "prepare": "husky install",
    "postinstall": "yarn yarn-deduplicate yarn.lock"
  },
