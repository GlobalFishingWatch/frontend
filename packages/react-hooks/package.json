    "clean": "rimraf dist/*",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@globalfishingwatch/api-client": "^1.5.1",
    "@globalfishingwatch/dataviews-client": "^3.2.0",
    "@globalfishingwatch/layer-composer": "^2.14.1",
    "@globalfishingwatch/mapbox-gl": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
