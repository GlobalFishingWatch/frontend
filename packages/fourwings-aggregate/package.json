{
  "name": "@globalfishingwatch/fourwings-aggregate",
  "version": "1.1.3",
  "description": "Logic to turn fourwings tiles or cells into meaningful values for the frontend",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "satellitestudio <contact@satellitestud.io>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GlobalFishingWatch/frontend.git"
  },
  "bugs": {
    "url": "https://github.com/GlobalFishingWatch/frontend/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
