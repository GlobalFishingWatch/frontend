{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "shippedProposals": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "chrome": 61,
              "node": 12
            }
          }
        ]
      ]
    }
  }
}
