{
  "extends": "../../config/tsconfig.json",
  "include": [
    "src/"
  ],
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist/",
    "typeRoots": [
      "../../node_modules/@types"
    ],
    "declarationDir": "dist",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  }
}
