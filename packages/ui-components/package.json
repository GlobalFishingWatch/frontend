{
  "name": "@globalfishingwatch/ui-components",
  "version": "1.1.6",
  "description": "Set of react components to consume on the gfw ecosystem",
  "author": "satellitestudio <contact@satellitestud.io>",
  "homepage": "https://github.com/GlobalFishingWatch/frontend#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "src": "src",
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GlobalFishingWatch/frontend.git"
  },
  "scripts": {
    "start": "rollup -c ./rollup.config.js -w",
    "build": "yarn clean && cross-env NODE_ENV=production rollup -c rollup.config.js",
    "clean": "rimraf dist/*",
    "prerender:build": "ts-node -P tsconfig.tsnode.json src/header/render-html.tsx",
    "prerender:copy": "cp -r src/header/html dist/header",
    "prepublishOnly": "yarn build && yarn prerender:build && yarn prerender:copy"
  },
  "dependencies": {
    "@globalfishingwatch/ui-kit": "*",
    "countryflag": "3.2.2",
    "d3-geo": "1.12.0",
    "topojson-client": "3.1.0"
  },
  "devDependencies": {
    "@types/topojson-client": "^3.0.0",
    "ts-node": "^8.10.1"
  },
  "gitHead": "10c3825a90f584a1c240e6f3c1a77dece200a650"
}
