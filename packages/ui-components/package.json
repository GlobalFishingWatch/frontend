{
  "name": "@globalfishingwatch/ui-components",
  "version": "11.1.2",
  "description": "Set of react components to consume on the gfw ecosystem",
  "author": "satellitestudio <contact@satellitestud.io>",
  "homepage": "https://github.com/GlobalFishingWatch/frontend#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "src": "src",
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GlobalFishingWatch/frontend.git"
  },
  "scripts": {
    "start": "run-p prerender start:js start:css",
    "start:js": "rollup -c ./rollup.config.js -w",
    "start:css": "postcss src/base.css -o dist/base.css -w",
    "build": "yarn clean && run-p build:js build:css build:css-min",
    "build:js": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "build:css": "postcss src/base.css -o dist/base.css",
    "build:css-min": "cross-env NODE_ENV=production postcss src/base.css -o dist/base.min.css ",
    "prerender": "run-s prerender:build prerender:copy",
    "prerender:build": "ts-node -P tsconfig.tsnode.json src/header/render-html.tsx",
    "prerender:copy": "cp -r src/header/html dist/header",
    "prepare": "yarn build && yarn prerender",
    "clean": "rimraf dist/*"
  },
  "dependencies": {
    "@tippyjs/react": "4.2.0",
    "countryflag": "4.0.1",
    "d3-geo": "1.12.0",
    "d3-scale": "^3.2.2",
    "match-sorter": "^6.3.0",
    "react-modal": "^3.11.2",
    "topojson-client": "3.1.0"
  },
  "devDependencies": {
    "@globalfishingwatch/layer-composer": "9.3.0",
    "@globalfishingwatch/react-hooks": "^14.0.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.0",
    "@rollup/plugin-url": "^6.1.0",
    "@svgr/rollup": "^5.4.0",
    "@types/react-modal": "^3.10.5",
    "@types/topojson-client": "^3.0.0",
    "autoprefixer": "^9.8.0",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-modules": "^2.0.0",
    "postcss-nested": "^4.2.1",
    "rollup": "^2.33.1",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^8.10.1"
  },
  "peerDependencies": {
    "@globalfishingwatch/layer-composer": "^9.0.0",
    "@globalfishingwatch/react-hooks": "^14.0.0",
    "downshift": "^6.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "dd9006c455eb7ee9f20cbdc62854d9a16bfc94c1"
}
