{
  "name": "tile-inspector",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/tile-inspector",
  "projectType": "application",
  "targets": {
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx build tile-inspector --parallel",
          "nx docker-prepare tile-inspector"
        ],
        "parallel": false
      }
    },
    "docker-prepare": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          "cp config/entrypoint.sh dist/apps/tile-inspector",
          "cp apps/tile-inspector/nginx.conf dist/apps/tile-inspector"
        ]
      }
    },
    "compile": {
      "executor": "@nx/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/tile-inspector"
      },
      "configurations": {
        "production": {}
      }
    },
    "start": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "tile-inspector:compile",
        "dev": true,
        "port": 3004
      },
      "configurations": {
        "production": {
          "buildTarget": "tile-inspector:compile:production",
          "dev": false
        }
      }
    },
    "build": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "tile-inspector:compile:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/apps/tile-inspector"
      ],
      "options": {
        "jestConfig": "apps/tile-inspector/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/tile-inspector/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
