{
  "name": "vessel-history",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/vessel-history",
  "projectType": "application",
  "targets": {
    "changelog": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["./changelog.sh vessel-history \"Vessel Viewer\""],
        "parallel": false
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx build vessel-history"],
        "parallel": false
      }
    },
    "compile": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/vessel-history",
        "outputPath": "dist/apps/vessel-history"
      },
      "configurations": {
        "production": {}
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx clean-locales vessel-history",
          "nx serve i18n-labels",
          "nx serve vessel-history"
        ],
        "parallel": true
      }
    },
    "clean-locales": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": ["rm -rf dist/apps/vessel-history/public/locales"]
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "vessel-history:compile",
        "dev": true,
        "port": 3000
      },
      "configurations": {
        "production": {
          "buildTarget": "vessel-history:compile:production",
          "dev": false
        }
      }
    },
    "build": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "vessel-history:compile:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/vessel-history"],
      "options": {
        "jestConfig": "apps/vessel-history/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/vessel-history/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
