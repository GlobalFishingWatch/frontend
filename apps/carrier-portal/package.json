    "serve": "npm start",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src -c .eslintrc.js --ext .js,jsx,ts,tsx --fix",
    "eject": "react-scripts eject",
    "test": "craco test"
  },
