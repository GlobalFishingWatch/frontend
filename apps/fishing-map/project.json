{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/fishing-map",
  "projectType": "application",
  "targets": {
    "changelog": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["./changelog.sh fishing-map \"Map\""],
        "parallel": false
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx build fishing-map --parallel", "nx docker-prepare fishing-map"],
        "parallel": false
      }
    },
    "docker-prepare": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          "cp config/entrypoint.sh dist/apps/fishing-map",
          "cp apps/fishing-map/nginx.conf dist/apps/fishing-map"
        ]
      }
    },
    "compile": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/fishing-map",
        "outputPath": "dist/apps/fishing-map"
      },
      "configurations": {
        "production": {}
      }
    },
    "start": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx clean-locales fishing-map",
          "nx serve i18n-labels",
          "nx serve fishing-map"
        ],
        "parallel": true
      }
    },
    "clean-locales": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "commands": ["rm -rf dist/apps/fishing-map/public/locales"]
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "fishing-map:compile",
        "dev": true,
        "port": 3003
      },
      "configurations": {
        "production": {
          "buildTarget": "fishing-map:compile:production",
          "dev": false
        }
      }
    },
    "build": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "fishing-map:compile:production"
      }
    },
    "rename-export": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "rm -rf dist/apps/fishing-map/map",
          "mv dist/apps/fishing-map/exported dist/apps/fishing-map/map"
        ],
        "parallel": false
      }
    },
    "serve:export": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx build fishing-map --parallel",
          "nx rename-export fishing-map",
          "serve dist/apps/fishing-map"
        ],
        "parallel": false
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/fishing-map"],
      "options": {
        "jestConfig": "apps/fishing-map/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/fishing-map/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
