{
  "name": "@globalfishingwatchapp/fishing-map",
  "version": "2.3.4",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint . -c .eslintrc.js --ext .js,jsx,ts,tsx --fix",
    "lint:css": "stylelint ./**/*.{css,scss,sass} --config .stylelintrc.js --fix",
    "i18n:parser": "i18next",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "devDependencies": {
    "i18next-parser": "8.13.0",
    "source-map-explorer": "2.5.3"
  },
  "browserslist": [
    ">0.1%",
    "not dead",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "@globalfishingwatch/linting"
    ],
    "ignorePatterns": "public/"
  },
  "prettier": "@globalfishingwatch/linting/prettier"
}
