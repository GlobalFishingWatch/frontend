{
  "name": "fishing-map-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/fishing-map-e2e/src",
  "projectType": "application",
  "tags": ["e2e"],
  "implicitDependencies": ["fishing-map"],
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "options": {
        "config": "apps/fishing-map-e2e/playwright.config.mts"
      },
      "cache": true,
      "inputs": ["default", "^production"],
      "outputs": ["{workspaceRoot}/dist/.playwright/apps/fishing-map-e2e"]
    },
    "e2e:local": {
      "executor": "@nx/playwright:playwright",
      "options": {
        "config": "apps/fishing-map-e2e/playwright.config.mts"
      },
      "cache": false,
      "inputs": ["default", "^production"],
      "outputs": ["{workspaceRoot}/dist/.playwright/apps/fishing-map-e2e"],
      "dependsOn": [
        {
          "projects": "fishing-map",
          "target": "start"
        }
      ],
      "configurations": {
        "ui": {
          "cache": false,
          "ui": true
        },
        "headed": {
          "cache": false,
          "headed": true
        }
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project apps/fishing-map-e2e/tsconfig.json"
      }
    },
    "typecheck-watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --watch --project apps/fishing-map-e2e/tsconfig.json"
      },
      "continuous": true
    }
  }
}
