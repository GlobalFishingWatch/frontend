{
  "sourceRoot": "apps/fishing-map-e2e/src",
  "projectType": "application",
  "targets": {
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["nx serve i18n-labels", "nx e2e fishing-map-e2e"],
        "parallel": true
      }
    },
    "test-dev": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/fishing-map-e2e/cypress.json",
        "baseUrl": "https://user:password@fishing-map.dev.globalfishingwatch.org"
      }
    },
    "test-pro": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/fishing-map-e2e/cypress.json",
        "baseUrl": "https://globalfishingwatch.org/map"
      }
    },
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/fishing-map-e2e/cypress.json",
        "devServerTarget": "fishing-map:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "fishing-map:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/fishing-map-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["fishing-map"]
}
