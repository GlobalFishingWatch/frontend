{
  "name": "user-groups-admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/user-groups-admin",
  "projectType": "application",
  "targets": {
    "changelog": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["./changelog.sh user-groups-admin \"User groups admin\""],
        "parallel": false
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx build user-groups-admin --parallel",
          "nx docker-prepare user-groups-admin"
        ],
        "parallel": false
      }
    },
    "docker-prepare": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": [
          "cp config/entrypoint.sh dist/apps/user-groups-admin",
          "cp apps/user-groups-admin/nginx.conf dist/apps/user-groups-admin"
        ]
      }
    },
    "compile": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/user-groups-admin",
        "outputPath": "dist/apps/user-groups-admin"
      },
      "configurations": {
        "production": {}
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx serve user-groups-admin"],
        "parallel": true
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "user-groups-admin:compile",
        "dev": true,
        "port": 3003
      },
      "configurations": {
        "production": {
          "buildTarget": "user-groups-admin:build:production",
          "dev": false
        }
      }
    },
    "build": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "user-groups-admin:compile:production"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/user-groups-admin/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/user-groups-admin"],
      "options": {
        "jestConfig": "apps/user-groups-admin/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
