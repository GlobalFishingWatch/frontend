{
  "name": "port-labeler",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/port-labeler",
  "projectType": "application",
  "targets": {
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx build port-labeler --parallel"],
        "parallel": false
      }
    },
    "compile": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/port-labeler",
        "outputPath": "dist/apps/port-labeler"
      },
      "configurations": {
        "production": {}
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx clean-locales port-labeler",
          "nx serve i18n-labels",
          "nx serve port-labeler"
        ],
        "parallel": true
      }
    },
    "clean-locales": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "commands": ["rm -rf dist/apps/port-labeler/public/locales"]
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "port-labeler:compile",
        "dev": true,
        "port": 3003
      },
      "configurations": {
        "production": {
          "buildTarget": "port-labeler:compile:production",
          "dev": false
        }
      }
    },
    "build": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "port-labeler:compile:production"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/port-labeler/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/port-labeler"],
      "options": {
        "jestConfig": "apps/port-labeler/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
