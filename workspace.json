    "fishing-map": {
      "root": "apps/fishing-map",
      "sourceRoot": "apps/fishing-map",
      "projectType": "application",
      "targets": {
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx build fishing-map --parallel", "nx docker-prepare fishing-map"],
            "parallel": false
          }
        },
        "docker-prepare": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "commands": [
              "cp config/entrypoint.sh dist/apps/fishing-map",
              "cp apps/fishing-map/nginx.conf dist/apps/fishing-map"
            ]
          }
        },
        "compile": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": "production",
          "options": {
            "root": "apps/fishing-map",
            "outputPath": "dist/apps/fishing-map"
          },
          "configurations": {
            "production": {}
          }
        },
        "start": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx clean-locales fishing-map",
              "nx serve i18n-labels",
              "nx serve fishing-map"
            ],
            "parallel": true
          }
        },
        "clean-locales": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "commands": ["rm -rf dist/apps/fishing-map/public/locales"]
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "fishing-map:compile",
            "dev": true,
            "port": 3003
          },
          "configurations": {
            "production": {
              "buildTarget": "fishing-map:compile:production",
              "dev": false
            }
          }
        },
        "build": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "fishing-map:compile:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/fishing-map"],
          "options": {
            "jestConfig": "apps/fishing-map/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/fishing-map/**/*.{ts,tsx,js,jsx}"]
          }
        }
      },
      "tags": []
    },
    "fishing-map-e2e": {
      "root": "apps/fishing-map-e2e",
      "sourceRoot": "apps/fishing-map-e2e/src",
      "projectType": "application",
      "targets": {
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx serve i18n-labels", "nx e2e fishing-map-e2e"],
            "parallel": true
          }
        },
        "test-dev": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/fishing-map-e2e/cypress.json",
            "baseUrl": "https://user:password@fishing-map.dev.globalfishingwatch.org"
          }
        },
        "test-pro": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/fishing-map-e2e/cypress.json",
            "baseUrl": "https://globalfishingwatch.org/map"
          }
        },
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/fishing-map-e2e/cypress.json",
            "devServerTarget": "fishing-map:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fishing-map:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/fishing-map-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["fishing-map"]
    },
