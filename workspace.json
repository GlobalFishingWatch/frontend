    "fishing-map": {
      "root": "applications/fishing-map",
      "sourceRoot": "applications/fishing-map/src",
      "projectType": "application",
      "targets": {
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx build fishing-map", "nx docker-prepare fishing-map"],
            "parallel": false
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": ["{options.outputPath}"],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired build",
            "cwd": "applications/fishing-map",
            "outputPath": "dist/applications/fishing-map"
          }
        },
        "docker-prepare": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "commands": [
              "cp config/entrypoint.sh dist/applications/fishing-map",
              "cp applications/fishing-map/nginx.conf dist/applications/fishing-map"
            ]
          }
        },
        "docker": {
          "executor": "@nx-tools/nx-docker:build",
          "options": {
            "push": false,
            "tags": ["gfw/fishing-map:latest"],
            "file": "./applications/fishing-map/Dockerfile",
            "context": "./dist/applications/fishing-map"
          }
        },
        "start": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired start",
            "cwd": "applications/fishing-map"
          }
        },
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/eslint src/**/*.tsx src/**/*.ts",
            "cwd": "applications/fishing-map"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired test --watchAll=false",
            "cwd": "applications/fishing-map"
          }
        }
      }
    },
    "fishing-map-e2e": {
      "root": "applications/fishing-map-e2e",
      "sourceRoot": "applications/fishing-map-e2e/src",
      "projectType": "application",
      "targets": {
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx e2e-serve fishing-map-e2e", "nx e2e-run fishing-map-e2e"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["applications/fishing-map-e2e/**/*.{js,ts}"]
          }
        },
        "e2e-run": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "applications/fishing-map-e2e/cypress.json",
            "tsConfig": "applications/fishing-map-e2e/tsconfig.json",
            "baseUrl": "http://localhost:3000"
          }
        },
        "e2e-serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx start fishing-map"],
            "readyWhen": "can now view"
          }
        }
      }
    },
