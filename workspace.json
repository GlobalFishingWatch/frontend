    "api-client": {
      "root": "packages/api-client",
      "sourceRoot": "packages/api-client/src",
      "projectType": "library",
      "targets": {
        "dist": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/api-client",
            "tsConfig": "packages/api-client/tsconfig.lib.json",
            "project": "packages/api-client/package.json",
            "entryFile": "packages/api-client/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "packages/api-client/rollup.config.js",
            "assets": [
              {
                "glob": "packages/api-client/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/api-client/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/api-client"],
          "options": {
            "jestConfig": "packages/api-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
