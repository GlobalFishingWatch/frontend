{
  "affected": {
    "defaultBase": "develop"
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "runtimeCacheInputs": [
          "echo $NEXT_PUBLIC_URL",
          "echo $NEXT_PUBLIC_API_GATEWAY",
          "echo $NEXT_PUBLIC_WORKSPACE_ENV",
          "echo $NEXT_PUBLIC_GOOGLE_MEASUREMENT_ID",
          "echo $NEXT_PUBLIC_GOOGLE_TAG_MANAGER_KEY",
          "echo $NEXT_PUBLIC_GOOGLE_ANALYTICS_TEST_MODE",
          "echo $NEXT_PUBLIC_CARRIER_PORTAL_URL",
          "echo $NEXT_PUBLIC_LATEST_CARRIER_DATASET_ID",
          "echo $NEXT_PUBLIC_MAP_ERRORS_SPREADSHEET_ID",
          "echo $NEXT_PUBLIC_FEEDBACK_SPREADSHEET_ID",
          "echo $NEXT_PUBLIC_SPREADSHEET_CLIENT_EMAIL",
          "echo $NEXT_PUBLIC_SPREADSHEET_PRIVATE_KEY"
        ],
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "generators": {
    "@nx/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nx/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "fishing-map",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "cache": true
    },
    "e2e": {
      "inputs": ["default", "^production"],
      "cache": true
    },
    "test": {
      "inputs": ["default", "^production", "{workspaceRoot}/jest.preset.js"],
      "cache": true
    },
    "@nx/jest:jest": {
      "inputs": ["default", "^production", "{workspaceRoot}/jest.preset.js"],
      "cache": true,
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.js"],
      "cache": true
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": ["{workspaceRoot}/nx.json", "{workspaceRoot}/babel.config.json"],
    "production": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/.eslintrc.js",
      "!{projectRoot}/src/test-setup.[jt]s"
    ]
  },
  "nxCloudAccessToken": "NzEyZWVlMmMtZjQ3OS00YzMzLWE2YzAtYjJmMDE1YzcxNjllfHJlYWQtd3JpdGU="
}
