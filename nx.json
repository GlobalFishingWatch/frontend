{
  "npmScope": "globalfishingwatch",
  "affected": {
    "defaultBase": "monorepo-update"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e", "deploy"],
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api-client": {
      "implicitDependencies": []
    },
    "api-types": {
      "implicitDependencies": []
    },
    "data-transforms": {
      "implicitDependencies": []
    },
    "dataviews-client": {
      "implicitDependencies": []
    },
    "fishing-map": {
      "implicitDependencies": []
    },
    "fishing-map-e2e": {
      "implicitDependencies": ["fishing-map"]
    },
    "fourwings-aggregate": {
      "implicitDependencies": []
    },
    "i18n-labels": {},
    "layer-composer": {
      "implicitDependencies": []
    },
    "ocean-areas": {
      "implicitDependencies": []
    },
    "pbf": {
      "implicitDependencies": []
    },
    "react-hooks": {
      "implicitDependencies": []
    },
    "timebar": {
      "implicitDependencies": []
    },
    "ui-components": {
      "implicitDependencies": []
    }
  }
}
