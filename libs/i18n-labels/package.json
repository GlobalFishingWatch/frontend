{
  "name": "@globalfishingwatch/i18n-labels",
  "description": "i18n label translations for Global Fishing Watch applications",
  "homepage": "https://github.com/GlobalFishingWatch/frontend#readme",
  "version": "1.2.285",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "ts:lint": "npx prettier ./resources.d.ts --write",
    "ts:generate": "i18next-resources-for-ts interface -i ./source -o ./resources.d.ts",
    "ts": "yarn ts:generate && yarn ts:lint"
  },
  "devDependencies": {
    "i18next-resources-for-ts": "1.7.4"
  },
  "files": [
    "en",
    "es",
    "fr",
    "id",
    "pt",
    "source",
    "val"
  ],
  "dependencies": {}
}
