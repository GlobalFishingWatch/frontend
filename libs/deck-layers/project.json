{
  "name": "deck-layers",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/deck-layers/src",
  "projectType": "library",
  "targets": {
    "dist": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "libs/deck-layers/dist",
        "main": "libs/deck-layers/src/index.ts",
        "tsConfig": "libs/deck-layers/tsconfig.lib.json",
        "rootDir": "libs/deck-layers/src"
      }
    },
    "publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": [
        "dist"
      ],
      "options": {
        "packageRoot": "libs/deck-layers/dist"
      }
    }
  },
  "tags": []
}
