{
  "name": "@globalfishingwatch/ocean-areas",
  "version": "2.0.3",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "mpas": "ts-node -P tsconfig.json src/scripts/mpas.ts",
    "fao": "ts-node -P tsconfig.json src/scripts/fao.ts",
    "eezs": "ts-node -P tsconfig.json src/scripts/eezs.ts",
    "rfmos": "ts-node -P tsconfig.json src/scripts/rfmos.ts",
    "locales": "ts-node -P tsconfig.json src/scripts/locales.ts",
    "ports": "ts-node -P tsconfig.json src/scripts/ports.ts",
    "prepare": "yarn mpas && yarn eezs && yarn rfmos && yarn fao && yarn locales"
  },
  "dependencies": {
    "@turf/bbox": "7.x",
    "@turf/bbox-polygon": "7.x",
    "@turf/boolean-point-in-polygon": "7.x",
    "@turf/distance": "7.x",
    "@turf/explode": "7.x",
    "@turf/helpers": "7.x",
    "@turf/nearest-point": "7.x",
    "@turf/simplify": "7.x",
    "@turf/truncate": "7.x",
    "dotenv": "17.x",
    "es-toolkit": "1.x",
    "geojson": "0.x",
    "match-sorter": "8.x",
    "tslib": "2.x"
  },
  "devDependencies": {
    "@types/lodash": "4.x",
    "@types/node": "20.x",
    "dotenv": "^17.2.0",
    "ts-node": "10.x",
    "typescript": "5.x"
  }
}
