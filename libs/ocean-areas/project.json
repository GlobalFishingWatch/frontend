{
  "name": "ocean-areas",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ocean-areas/src",
  "projectType": "library",
  "targets": {
    "dist": {
      "executor": "@nx/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "assets": [],
        "compiler": "tsc",
        "format": [
          "esm"
        ],
        "main": "libs/ocean-areas/src/index.ts",
        "outputPath": "dist/libs/ocean-areas",
        "project": "libs/ocean-areas/package.json",
        "tsConfig": "libs/ocean-areas/tsconfig.lib.json"
      }
    },
    "publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": [
        "dist-package"
      ],
      "options": {
        "packageRoot": "dist/libs/ocean-areas"
      }
    },
    "dist-package": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "dist"
      ],
      "outputs": [
        "{workspaceRoot}/dist/libs/ocean-areas/package.json"
      ],
      "options": {
        "command": "node tools/scripts/generate-dist-package.mjs libs/ocean-areas dist/libs/ocean-areas"
      }
    }
  },
  "tags": []
}
