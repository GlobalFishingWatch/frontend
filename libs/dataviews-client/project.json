{
  "name": "dataviews-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dataviews-client/src",
  "projectType": "library",
  "targets": {
    "dist": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/dataviews-client",
        "main": "libs/dataviews-client/src/index.ts",
        "tsConfig": "libs/dataviews-client/tsconfig.lib.json",
        "assets": [
          {
            "input": "libs/dataviews-client",
            "glob": "package.json",
            "output": "."
          },
          {
            "input": "libs/dataviews-client",
            "glob": "README.md",
            "output": "."
          }
        ],
        "rootDir": "libs/dataviews-client/src"
      }
    },
    "bundle-url-workspace": {
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/dist/libs/dataviews-client/url-workspace-bundle"
      ],
      "dependsOn": [],
      "options": {
        "commands": [
          "node libs/dataviews-client/esbuild.url.ts"
        ],
        "cwd": "."
      }
    },
    "publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": [
        "dist-package"
      ],
      "options": {
        "packageRoot": "dist/libs/dataviews-client"
      }
    },
    "dist-package": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "dist"
      ],
      "outputs": [
        "{workspaceRoot}/dist/libs/dataviews-client/package.json"
      ],
      "options": {
        "command": "node tools/scripts/generate-dist-package.mjs libs/dataviews-client dist/libs/dataviews-client"
      }
    }
  },
  "tags": []
}
