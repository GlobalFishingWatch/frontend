{
  "name": "timebar",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/timebar/src",
  "projectType": "library",
  "targets": {
    "dist": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "libs/timebar/dist",
        "main": "libs/timebar/src/index.ts",
        "tsConfig": "libs/timebar/tsconfig.lib.json",
        "assets": [
          {
            "glob": "**/*.css",
            "input": "libs/timebar/src",
            "output": "."
          }
        ],
        "rootDir": "libs/timebar/src"
      }
    },
    "publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": [
        "dist"
      ],
      "options": {
        "packageRoot": "libs/timebar/dist"
      }
    }
  },
  "tags": []
}
