{
  "name": "pbf-decoders",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/pbf-decoders",
  "projectType": "library",
  "targets": {
    "dist": {
      "executor": "@nx/js:tsc",
      "options": {
        "main": "libs/pbf-decoders/index.ts",
        "rootDir": "libs/pbf-decoders",
        "generatePackageJson": true,
        "assets": [
          { "glob": "**/*", "input": "libs/pbf-decoders/decoders", "output": "decoders" }
        ]
      }
    },
    "publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": ["dist"],
      "options": {
        "packageRoot": "libs/pbf-decoders"
      }
    }
  },
  "tags": []
}
