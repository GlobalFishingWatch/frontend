{
  "name": "pbf-decoders",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/pbf-decoders",
  "projectType": "library",
  "targets": {
    "dist": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/pbf-decoders",
        "main": "libs/pbf-decoders/index.ts",
        "tsConfig": "libs/pbf-decoders/tsconfig.lib.json",
        "assets": [
          "libs/pbf-decoders/package.json",
          "libs/pbf-decoders/README.md",
          { "glob": "**/*", "input": "libs/pbf-decoders/decoders", "output": "decoders" }
        ],
        "rootDir": "libs/pbf-decoders"
      }
    },
    "publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": ["dist"],
      "options": {
        "packageRoot": "dist/libs/pbf-decoders"
      }
    }
  },
  "tags": []
}
