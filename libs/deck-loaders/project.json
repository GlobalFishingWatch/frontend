{
  "name": "deck-loaders",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/deck-loaders/src",
  "projectType": "library",
  "targets": {
    "dist:lib": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "assets": [],
        "compiler": "swc",
        "format": ["esm", "cjs"],
        "main": "libs/deck-loaders/src/index.ts",
        "outputPath": "dist/libs/deck-loaders",
        "project": "libs/deck-loaders/package.json",
        "tsConfig": "libs/deck-loaders/tsconfig.lib.json"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/deck-loaders/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/deck-loaders"],
      "options": {
        "jestConfig": "libs/deck-loaders/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  }
}
