{
  "name": "deck-layer-composer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/deck-layer-composer/src",
  "projectType": "library",
  "targets": {
    "dist": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "libs/deck-layer-composer/dist",
        "generatePackageJson": false,
        "main": "libs/deck-layer-composer/src/index.ts",
        "tsConfig": "libs/deck-layer-composer/tsconfig.lib.json",
        "rootDir": "libs/deck-layer-composer/src"
      }
    },
    "publish": {
      "executor": "@nx/js:release-publish",
      "dependsOn": ["dist"],
      "options": {
        "packageRoot": "libs/deck-layer-composer"
      }
    }
  },
  "tags": []
}
