  "devDependencies": {
    "@craco/craco": "^6.3.0",
    "@globalfishingwatch/linting": "1.8.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@types/lodash": "^4.14.161",
    "@types/luxon": "^1.25.0",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-image-gallery": "^1.0.3",
    "@types/react-map-gl": "^5.2.7",
    "@types/react-redux": "^7.1.9",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/redux-first-router": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-eslint": "^10.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-flowtype": "5.9.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "fake-indexeddb": "^3.1.3",
    "husky": "^7.0.0",
    "i18next-parser": "^3.7.0",
    "lint-staged": "^11.1.0"
  },
  "resolutions": {
    "gl-matrix": "3.3.0",
    "reselect": "4.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@globalfishingwatch/linting/typescript"
    ],
    "ignorePatterns": "public/"
  },
  "prettier": "@globalfishingwatch/linting/prettier",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint -c ./.eslintrc.js --fix"
    ],
    "*.{css, scss}": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^workerize-loader(\\?.*)?!(.*)/([^/]*)$": "$2/__mocks__/$3"
    },
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/types/**/*.*"
    ]
  }
}
