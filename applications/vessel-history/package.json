    "luxon": "^1.25.0",
    "qs": "^6.9.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-i18next": "^11.8.12",
    "react-image-gallery": "^1.1.1",
    "react-map-gl": "^5.3.17",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.3",
    "react-virtualized-auto-sizer": "^1.0.5",
